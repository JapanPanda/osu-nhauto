<Window x:Name="osu_nhauto" x:Class="osu_nhauto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:osu_nhauto"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="600" Background="#FF2E2D2D">
    <Grid>
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.ColumnSpan="2">
            <TextBlock x:Name="statusWindow" Padding="10,5,0,0" Margin="10,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="241" Background="{DynamicResource {x:Static SystemColors.MenuBrushKey}}" FontSize="24"/>
        </StackPanel>
    </Grid>
</Window>
